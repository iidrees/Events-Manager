{
  "name": "events-manager",
  "version": "1.0.0",
  "description": "An Events Manager Application that allows users host their organised events",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test && nyc mocha server/test/test.js --require babel-register --exit",
    "test:env": "nyc mocha server/test/test.js --require babel-register --exit",
    "test:tester": "nyc cross-env NODE_ENV=test  --reporter=html --reporter=text --reporter=lcov --reporter=text-lcov _mocha server/test/test.js --compilers js:babel-core/register --exit",
    "start": "babel-node  server/src/server.js",
    "start:dev": "nodemon server/src/server.js --exec babel-node",
    "build": "babel server/src -d server/lib",
    "start:prod": "node server/lib/server.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "nyc": "nyc report",
    "seq:drop": "sequelize db:migrate:undo:all",
    "seq:create": "sequelize db:migrate",
    "seq:elecreate": "sequelize db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iidrees/Events-Manager.git"
  },
  "keywords": [
    "Node.js",
    "Express.js",
    "React.js",
    "API",
    "Events-Management"
  ],
  "author": "Idrees Ibraheem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iidrees/Events-Manager/issues"
  },
  "homepage": "https://github.com/iidrees/Events-Manager#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.11.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.0",
    "nyc": "^11.3.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.8",
    "sequelize-cli": "^3.1.0",
    "supertest": "^3.0.0",
    "validator": "^9.1.1"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "nodemon": "^1.12.1"
  }
}
