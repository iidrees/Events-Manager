{
  "name": "events-manager",
  "version": "1.0.0",
  "description":
    "An Events Manager Application that allows users host their organised events",
  "main": "server.js",
  "scripts": {
    "test":
      "npm run seq:db && nyc mocha server/test/* --timeout 25000 --require babel-register --exit",
    "test:env":
      "cross-env NODE_ENV=test npm run seq:db && nyc mocha server/test/* --timeout 25000 --require babel-register --exit",
    "test:tester":
      "cross-env NODE_ENV=test npm run seq:db && nyc --reporter=html --reporter=text --reporter=lcov --reporter=text-lcov _mocha server/test/ --compilers js:babel-core/register --exit",
    "start:prod": "NODE_ENV=production babel-node  server/src/server.js",
    "start:dev":
      "NODE_ENV=development nodemon server/src/server.js --exec babel-node",
    "build": "babel server/src -d server/lib",
    "heroku-postbuild": "npm run build",
    "start":
      "npm run build && node --optimize_for_size --max_old_space_size=920 --gc_interval=100 server/lib/server.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "nyc": "nyc report",
    "lint:client": "node_modules/.bin/eslint client/__tests__",
    "lint:server": "node_modules/.bin/eslint client/server/src/server.js",
    "seq:drop": "sequelize db:migrate:undo:all",
    "seq:create": "sequelize db:migrate",
    "seq:seed": "sequelize db:seed:all",
    "seq:unseed": "sequelize db:seed:undo:all",
    "seq:elecreate": "sequelize db:migrate",
    "seq:db": "npm run seq:drop && npm run seq:create && npm run seq:seed",
    "watch": "webpack -d && webpack-dev-server --hot --inline",
    "compile-server":
      "babel server --presets es2015 --out-dir client/dist --source-maps",
    "jest": "jest && codecov",
    "jest:cover": "jest --coverage",
    "jest:watch": "jest --watch"
  },
  "babel": {
    "presets": ["es2015", "react", "stage-0"],
    "plugins": ["transform-object-rest-spread"]
  },
  "jest": {
    "setupFiles": ["jest-localstorage-mock"],
    "modulePathIgnorePatterns": [
      "server/test/",
      "client/__tests__/actions/mocks/",
      "e2e/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iidrees/Events-Manager.git"
  },
  "keywords": ["Node.js", "Express.js", "React.js", "API", "Events-Management"],
  "author": "Idrees Ibraheem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iidrees/Events-Manager/issues"
  },
  "homepage": "https://github.com/iidrees/Events-Manager#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.5",
    "eslint": "^4.11.0",
    "expect": "^22.4.3",
    "express": "^4.16.2",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "image-webpack-loader": "^3.4.2",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "nodemailer": "^4.6.5",
    "nyc": "^11.3.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "rc-pagination": "^1.16.3",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^3.1.3",
    "react-moment": "^0.7.0",
    "react-preloading-component": "0.0.5",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.22.8",
    "sequelize-cli": "^3.1.0",
    "shortid": "^2.2.8",
    "style-loader": "^0.19.0",
    "supertest": "^3.0.0",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^3.0.8",
    "toastr": "^2.1.4",
    "validator": "^9.1.1",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "codecov": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.2.0",
    "jest-localstorage-mock": "^2.2.0",
    "moxios": "^0.4.0",
    "nodemon": "^1.12.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack-merge": "^4.1.2"
  }
}
